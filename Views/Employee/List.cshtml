@model List<Student.Portal.Mvc.Dipps.Models.Entities.Employee>
@{
}

<h1> Employee </h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th></th>

        </tr>
    </thead>
    <tbody>

        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Employee" asp-action="Edit" asp-route-id="@employee.Id"> Edit </a>
                </td>
            </tr>

        }
    </tbody>
</table>



