@model List<Student.Portal.Mvc.Dipps.Models.Entities.Employee>
@{
}

<h1> Employee </h1>

<table class="table">
   <thead>
       <tr>
           <th>Id</th>
           <th>Name</th>
           <th>Email</th>
           <th>Phone</th>
           <th>Subscribed</th>

       </tr>
   </thead>
   <tbody>

        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Phone</td>
                <td>@employee.Subscribed</td>
            </tr>

        }
    </tbody>
</table>